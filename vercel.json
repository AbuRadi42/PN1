{
  "version": 2,
  "builds": [
    {
      "src": "server.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.10",
        "startCommand": "sanic server:app --host 0.0.0.0 --port $PORT --workers 4 --access-log"
      }
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "server.py" }
  ]
}
